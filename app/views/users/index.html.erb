<h2>Users</h2>
<% @users.each do |user| %>
  <table>
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <% if user.profile_image.attached? %>
            <%= image_tag user.get_profile_image(80,80) %>
          <% else %>
            <%= image_tag "default-image_tag", width: 80, height: 80 %>
          <% end %>
        </td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
      </tr>
    </tbody>
  </table>
<% end %>
