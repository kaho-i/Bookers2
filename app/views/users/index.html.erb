<h2>Users</h2>
<% @users.each do |user| %>
  <table class="table">
    <thead>
      <tr>
        <th>image</th>
        <th>name</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>
          <%= image_tag user.get_profile_image(80,80) %>
          </td>
        <td><%= user.name %></td>
        <td><%= link_to 'Show', user_path(user.id) %></td>
      </tr>
    </tbody>
  </table>
<% end %>
